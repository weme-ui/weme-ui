{
  "$schema": "https://weme-ui.github.io/schema/project.json",
  "repos": [
    {
      "repo": "@weme-ui/weme-ui",
      "registry": "weme-ui/full",
      "default": true
    }
  ],
  "paths": {
    "components": "~/components-1",
    "composables": "~/composables-1",
    "layouts": "~/layouts-1",
    "pages": "~/pages-1",
    "plugins": "~/plugins-1",
    "themes": "~/themes-1",
    "types": "~/types-1",
    "utils": "~/utils-1"
  },
  "unocss": {
    "variablePrefix": "ui",
    "accentColors": {
      "ocean": "blue"
    },
    "neutralColors": {
      "gunmetal": "gray"
    },
    "cssVars": {
      "card-bg": "color.neutral.1",
      "card-fg": "color.neutral.11"
    }
  }
}
