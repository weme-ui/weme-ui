{
  "name": "@weme-ui/weme-ui",
  "type": "module",
  "version": "0.1.5",
  "packageManager": "pnpm@10.26.1",
  "description": "Re-usable UI components with Reka UI and UnoCSS.",
  "author": "Allen Luo <luoyi@mouji.net>",
  "license": "MIT",
  "homepage": "https://github.com/weme-ui/weme-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weme-ui/weme-ui.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/weme-ui/weme-ui/issues"
  },
  "keywords": [
    "weme-ui",
    "reusable",
    "ui-components",
    "reka-ui",
    "unocss"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.mts"
      ]
    }
  },
  "bin": "./dist/index.mjs",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "dev:prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@clack/core": "catalog:",
    "@clack/prompts": "catalog:",
    "@inquirer/prompts": "catalog:",
    "@weme-ui/schema": "workspace:*",
    "chalk": "catalog:",
    "citty": "catalog:",
    "columnify": "catalog:",
    "debug": "catalog:",
    "defu": "catalog:",
    "diff": "catalog:",
    "handlebars": "catalog:",
    "log-symbols": "catalog:",
    "make-fetch-happen": "catalog:",
    "mkdirp": "catalog:",
    "neverthrow": "catalog:",
    "nypm": "catalog:",
    "ora": "catalog:",
    "pathe": "catalog:",
    "pkg-types": "catalog:",
    "rfdc": "catalog:",
    "scule": "catalog:",
    "spark-md5": "catalog:",
    "tinyexec": "catalog:",
    "tinyglobby": "catalog:",
    "ufo": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/columnify": "catalog:",
    "@types/make-fetch-happen": "catalog:",
    "@types/node": "catalog:",
    "@types/spark-md5": "catalog:",
    "bumpp": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
