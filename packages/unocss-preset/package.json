{
  "name": "@weme-ui/unocss-preset",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@10.20.0",
  "description": "Weme UI - UnoCSS Preset",
  "author": "Allen Luo <luoyi@mouji.net>",
  "license": "MIT",
  "homepage": "https://github.com/weme-ui/weme-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weme-ui/weme-ui.git",
    "directory": "packages/unocss-preset"
  },
  "bugs": {
    "url": "https://github.com/weme-ui/weme-ui/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "weme-ui"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esno scripts/prepare.ts && tsdown",
    "dev": "tsdown --watch",
    "dev:prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsgo --noEmit",
    "play": "vite playground",
    "release": "pnpm build && bumpp"
  },
  "peerDependencies": {
    "@radix-ui/colors": "catalog:dev",
    "@unocss/rule-utils": "catalog:dev",
    "colorjs.io": "catalog:dev",
    "defu": "catalog:dev",
    "unocss": "catalog:dev"
  },
  "devDependencies": {
    "@radix-ui/colors": "catalog:dev",
    "@types/node": "catalog:build",
    "@unocss/rule-utils": "catalog:dev",
    "@vitejs/plugin-vue": "catalog:dev",
    "@vueuse/core": "catalog:registry",
    "@weme-ui/colors": "workspace:*",
    "@weme-ui/schema": "workspace:*",
    "bezier-easing": "catalog:dev",
    "bumpp": "catalog:build",
    "colorjs.io": "catalog:dev",
    "defu": "catalog:dev",
    "esno": "catalog:build",
    "reka-ui": "catalog:registry",
    "tsdown": "catalog:build",
    "typescript": "catalog:build",
    "unocss": "catalog:dev",
    "vite": "catalog:build",
    "vite-plugin-vue-devtools": "catalog:dev",
    "vitest": "catalog:testing",
    "vue": "catalog:dev",
    "vue-router": "catalog:dev"
  }
}
