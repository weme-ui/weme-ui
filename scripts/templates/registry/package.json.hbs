  "peerDependencies": {
    "@vueuse/core": "catalog:dev",
    "clsx": "catalog:dev",
    "nuxt": "catalog:nuxt",
    "reka-ui": "catalog:dev",
    "tailwind-merge": "catalog:dev",
    "tailwind-variants": "catalog:dev",
    "unocss": "catalog:dev",
    "vue": "catalog:vue"
  },
  "devDependencies": {
    "@nuxt/test-utils": "catalog:nuxt",
    "@vue/test-utils": "catalog:vue",
    "@vueuse/core": "catalog:dev",
    "clsx": "catalog:dev",
    "happy-dom": "catalog:testing",
    "nuxt": "catalog:nuxt",
    "playwright-core": "catalog:testing",
    "reka-ui": "catalog:dev",
    "tailwind-merge": "catalog:dev",
    "tailwind-variants": "catalog:dev",
    "unocss": "catalog:dev",
    "vue": "catalog:vue",
    "vue-tsc": "catalog:vue"
  }
