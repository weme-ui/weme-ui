{
  "name": "@weme-ui/registry-{{ name }}",
  "type": "module",
  "version": "{{ version }}",
  "private": true,
  "packageManager": "{{ packageManager }}",
  "description": "Weme UI components registry.",
  "author": "{{{ author }}}",
  "license": "{{ license }}",
  "homepage": "https://github.com/weme-ui/weme-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weme-ui/weme-ui.git",
    "directory": "registry/{{ name }}"
  },
  "bugs": {
    "url": "https://github.com/weme-ui/weme-ui/issues"
  },
  "scripts": {
