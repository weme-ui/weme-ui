{
  "name": "@weme-ui/registry-{{ kebabCase name }}",
  "type": "module",
  "version": "{{ version }}",
  "private": true,
  "packageManager": "{{ packageManager }}",
  "description": "Weme UI {{ titleCase name }} components registry.",
  "author": "{{{ author }}}",
  "license": "{{ license }}",
  "homepage": "https://github.com/weme-ui/weme-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weme-ui/weme-ui.git",
    "directory": "registry/{{ kebabCase name }}"
  },
  "bugs": {
    "url": "https://github.com/weme-ui/weme-ui/issues"
  },
  "keywords": [
    "vue",
    "nuxt",
    "unocss",
    "reka-ui",
    "radix-colors",
    "weme-ui",
    "weme-ui-registry",
    "weme-ui-{{ kebabCase name }}-registry",
    "vue-components",
    "nuxt-components",
    "reka-ui-components",
    "unocss-ui-components",
    "radix-colors-components"
  ],
  "scripts": {
    "dev": "nuxi dev .playground",
    "dev:prepare": "nuxi prepare .playground",
    "typecheck": "vue-tsc --noEmit && cd .playground && vue-tsc --noEmit"
  },
  "peerDependencies": {
    "@vueuse/core": "catalog:dev",
    "clsx": "catalog:dev",
    "nuxt": "catalog:nuxt",
    "reka-ui": "catalog:dev",
    "tailwind-merge": "catalog:dev",
    "tailwind-variants": "catalog:dev",
    "unocss": "catalog:dev",
    "vue": "catalog:vue"
  },
  "devDependencies": {
    "@nuxt/test-utils": "catalog:nuxt",
    "@vue/test-utils": "catalog:vue",
    "@vueuse/core": "catalog:dev",
    "@weme-ui/nuxt": "workspace:*",
    "@weme-ui/unocss-preset": "workspace:*",
    "clsx": "catalog:dev",
    "happy-dom": "catalog:testing",
    "nuxt": "catalog:nuxt",
    "playwright-core": "catalog:testing",
    "reka-ui": "catalog:dev",
    "tailwind-merge": "catalog:dev",
    "tailwind-variants": "catalog:dev",
    "unocss": "catalog:dev",
    "vue": "catalog:vue",
    "vue-tsc": "catalog:vue"
  }
}
