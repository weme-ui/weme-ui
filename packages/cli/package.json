{
  "name": "@weme-ui/weme-ui",
  "type": "module",
  "version": "0.1.5",
  "packageManager": "pnpm@10.25.0",
  "description": "Re-usable UI components with Reka UI and UnoCSS.",
  "author": "Allen Luo <luoyi@mouji.net>",
  "license": "MIT",
  "homepage": "https://github.com/weme-ui/weme-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weme-ui/weme-ui.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/weme-ui/weme-ui/issues"
  },
  "keywords": [
    "weme-ui",
    "reusable",
    "ui-components",
    "reka-ui",
    "unocss"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.mts"
      ]
    }
  },
  "bin": "./dist/index.mjs",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "dev:prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@clack/core": "catalog:utils",
    "@clack/prompts": "catalog:utils",
    "@inquirer/prompts": "catalog:utils",
    "@weme-ui/schema": "workspace:*",
    "chalk": "catalog:utils",
    "citty": "catalog:utils",
    "columnify": "catalog:utils",
    "debug": "catalog:utils",
    "defu": "catalog:dev",
    "diff": "catalog:utils",
    "handlebars": "catalog:utils",
    "log-symbols": "catalog:utils",
    "make-fetch-happen": "catalog:utils",
    "mkdirp": "catalog:utils",
    "neverthrow": "catalog:utils",
    "nypm": "catalog:utils",
    "ora": "catalog:utils",
    "pathe": "catalog:utils",
    "pkg-types": "catalog:utils",
    "rfdc": "catalog:utils",
    "scule": "catalog:utils",
    "spark-md5": "catalog:utils",
    "tinyexec": "catalog:utils",
    "tinyglobby": "catalog:utils",
    "ufo": "catalog:utils",
    "zod": "catalog:utils"
  },
  "devDependencies": {
    "@types/columnify": "catalog:utils",
    "@types/make-fetch-happen": "catalog:utils",
    "@types/node": "catalog:build",
    "@types/spark-md5": "catalog:utils",
    "bumpp": "catalog:build",
    "tsdown": "catalog:build",
    "typescript": "catalog:build",
    "vitest": "catalog:testing"
  }
}
