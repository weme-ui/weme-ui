{
  "$schema": "https://weme-ui.github.io/schema/project.schema.json",
  "repos": [
    {
      "registry": "weme-ui/std",
      "prefix": "ui",
      "default": true
    }
  ],
  "paths": {
    "components": "~/components"
  },
  "items": {
    "https://github.com/weme-ui/weme-ui": {
      "weme-ui/std": [
        {
          "name": "foo",
          "type": "component",
          "path": "~/components/ui/foo"
        },
        {
          "name": "bar",
          "type": "component",
          "path": "~/components/ui/bar"
        }
      ]
    }
  }
}
