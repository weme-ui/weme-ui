{
  "$schema": "https://weme-ui.github.io/schema/project.json",
  "provider": "github",
  "repos": [
    {
      "repo": "@weme-ui/weme-ui",
      "registry": "weme-ui/std",
      "prefix": "ui"
    }
  ],
  "paths": {
    "components": "~/components",
    "composables": "~/composables",
    "layouts": "~/layouts",
    "themes": "~/themes",
    "plugins": "~/plugins",
    "utils": "~/utils",
    "types": "~/types"
  },
  "unocss": {
    "cssVars": {
      "card-background": "color.neutral.1",
      "card-foreground": "color.neutral.11"
    }
  }
}
