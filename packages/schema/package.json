{
  "name": "@weme-ui/schema",
  "type": "module",
  "version": "0.1.5",
  "packageManager": "pnpm@10.27.0",
  "description": "JSON Schemas utilities for Weme UI.",
  "author": "Allen Luo <luoyi@mouji.net>",
  "license": "MIT",
  "homepage": "https://weme-ui.github.io/schema",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weme-ui/weme-ui.git",
    "directory": "packages/schema"
  },
  "bugs": {
    "url": "https://github.com/weme-ui/weme-ui/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.13.0"
  },
  "scripts": {
    "build": "tsdown",
    "build:json": "esno ./src/output.ts",
    "dev": "tsdown --watch",
    "dev:prepare": "pnpm build",
    "test": "vitest"
  },
  "dependencies": {
    "mkdirp": "catalog:",
    "pathe": "catalog:",
    "scule": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "pkg-types": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
