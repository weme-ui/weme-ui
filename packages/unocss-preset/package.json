{
  "name": "@weme-ui/unocss-preset",
  "type": "module",
  "version": "0.1.5",
  "packageManager": "pnpm@10.26.2",
  "description": "Weme UI - UnoCSS Preset",
  "author": "Allen Luo <luoyi@mouji.net>",
  "license": "MIT",
  "homepage": "https://github.com/weme-ui/weme-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weme-ui/weme-ui.git",
    "directory": "packages/unocss-preset"
  },
  "bugs": {
    "url": "https://github.com/weme-ui/weme-ui/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "weme-ui"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esno scripts/prepare.ts && tsdown",
    "dev": "tsdown --watch",
    "dev:prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsgo --noEmit",
    "play": "vite playground"
  },
  "peerDependencies": {
    "@radix-ui/colors": "catalog:",
    "@unocss/core": "catalog:",
    "@unocss/rule-utils": "catalog:",
    "bezier-easing": "catalog:",
    "colorjs.io": "catalog:",
    "defu": "catalog:",
    "unocss": "catalog:"
  },
  "devDependencies": {
    "@radix-ui/colors": "catalog:",
    "@types/node": "catalog:",
    "@unocss/core": "catalog:",
    "@unocss/rule-utils": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vueuse/core": "catalog:",
    "@weme-ui/schema": "workspace:*",
    "bezier-easing": "catalog:",
    "bumpp": "catalog:",
    "colorjs.io": "catalog:",
    "defu": "catalog:",
    "esno": "catalog:",
    "reka-ui": "catalog:",
    "tsdown": "catalog:",
    "type-fest": "catalog:",
    "typescript": "catalog:",
    "unocss": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:"
  }
}
